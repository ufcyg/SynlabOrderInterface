<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="SynlabOrderInterface\Core\Content\CancelledConfirmation\OrderInterfaceCancelConfirmationDefinition">
            <tag name="shopware.entity.definition" entity="as_cancelled_confirmation" />
        </service>
        <service id="SynlabOrderInterface\Core\Content\StockQS\OrderInterfaceStockQSDefinition">
            <tag name="shopware.entity.definition" entity="as_stock_qs" />
        </service>


        <service id="SynlabOrderInterface\Core\Api\Utilities\OIOrderServiceUtils" public="true">      
            <argument type="service" id="Shopware\Core\Checkout\Order\SalesChannel\OrderService" />
        </service>

        <service id="SynlabOrderInterface\Core\Api\Utilities\OrderInterfaceMailServiceHelper" public="true">      
            <argument id="Shopware\Core\Content\MailTemplate\Service\MailService" type="service"/>
        </service>

        <service id="SynlabOrderInterface\Core\Api\Utilities\OrderInterfaceUtils" public="true">
            <argument id="SynlabOrderInterface\Core\Api\Utilities\OrderInterfaceRepositoryContainer" type="service" />
        </service>

        <service id="SynlabOrderInterface\Core\Api\Utilities\OrderInterfaceRepositoryContainer" public="true">
            <argument id="product_manufacturer_translation.repository" type="service" />  
            <argument id="product_translation.repository" type="service" /> 
            <argument id="country.repository" type="service" />  
            <argument id="unit_translation.repository" type="service" />  
        </service>

        <service id="SynlabOrderInterface\Core\Api\OrderInterfaceController" public="true">
            <argument type="service" id="Shopware\Core\System\SystemConfig\SystemConfigService" />
            <argument type="service" id="SynlabOrderInterface\Core\Api\Utilities\OrderInterfaceRepositoryContainer" />
            <argument type="service" id="SynlabOrderInterface\Core\Api\Utilities\OrderInterfaceUtils" />
            <argument type="service" id="SynlabOrderInterface\Core\Api\Utilities\OIOrderServiceUtils" />
            <argument type="service" id="SynlabOrderInterface\Core\Api\Utilities\OrderInterfaceMailServiceHelper" />
            <call method="setContainer">
                <argument type="service" id="service_container" />
            </call>
        </service>

        <service id="SynlabOrderInterface\ScheduledTask\ScheduledOrderTransferTask">
            <tag name="shopware.scheduled.task" />
        </service>
        <service id="SynlabOrderInterface\ScheduledTask\ScheduledOrderTransferTaskHandler">
            <argument type="service" id="scheduled_task.repository" />
            <argument type="service" id="SynlabOrderInterface\Core\Api\OrderInterfaceController" />

            <tag name="messenger.message_handler" />
        </service>

        <service id="SynlabOrderInterface\ScheduledTask\ScheduledOrderProcessTask">
            <tag name="shopware.scheduled.task" />
        </service>
        <service id="SynlabOrderInterface\ScheduledTask\ScheduledOrderProcessTaskHandler">
            <argument type="service" id="scheduled_task.repository" />
            <argument type="service" id="SynlabOrderInterface\Core\Api\OrderInterfaceController" />

            <tag name="messenger.message_handler" />
        </service>

        
    </services>
</container>